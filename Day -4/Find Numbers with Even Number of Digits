class Solution {
    public int findNumbers(int[] nums) {
        int n = nums.length;
        int cnt = 0;
        for(int i  = 0; i < n ; i++){
            if((nums[i] > 9 && nums[i] <100) || (nums[i] > 999 && nums[i] < 10000) || nums[i] == 100000){ // problem constraint is given so here this is enough    
                cnt++;
            }
        }
        return cnt;
        
    }
}
